{
  "name": "@exuanbo/file-icons-js",
  "version": "3.3.0",
  "description": "File-specific icons for front-end project, extracted from file-icons/atom",
  "main": "./dist/js/file-icons.js",
  "module": "./dist/js/file-icons.esm.js",
  "browser": "./dist/js/file-icons.min.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run format:check && rimraf coverage && jest",
    "pretest": "npm run build",
    "build": "./bin/compile && gulp",
    "docs": "markdownlint README.md && markdown-link-check README.md",
    "format": "eslint --fix '**/*.js'",
    "format:check": "eslint '**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exuanbo/file-icons-js.git"
  },
  "keywords": [
    "icon",
    "font",
    "icon font",
    "file icon",
    "font-awesome",
    "mfizz",
    "devicons",
    "file-icons",
    "octicons",
    "css",
    "less",
    "woff2"
  ],
  "author": {
    "name": "exuanbo",
    "email": "exuanbo@protonmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/exuanbo/file-icons-js/issues"
  },
  "homepage": "https://github.com/exuanbo/file-icons-js#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/stream": "1.1.0",
    "browserify": "16.5.2",
    "coffee-script": "1.12.7",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-config-prettier-standard": "3.0.1",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "get-options": "1.2.0",
    "gulp": "4.0.2",
    "gulp-clean-css": "4.3.0",
    "gulp-less": "4.0.1",
    "gulp-rename": "2.0.0",
    "gulp-replace": "1.0.0",
    "gulp-uglify-es": "2.0.0",
    "jest": "26.5.3",
    "markdown-link-check": "3.8.3",
    "markdownlint-cli": "0.24.0",
    "prettier": "2.1.2",
    "prettier-config-standard": "1.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.30.0",
    "vinyl-buffer": "1.0.1",
    "vinyl-source-stream": "2.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage"
  }
}
